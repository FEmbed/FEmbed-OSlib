{
    "name": "FEmbed-OSlib",
    "keywords": "FEmbed, Freertos, OS, RTOS",
    "description": "Arduino library for OS common library with FreeRTOS",
    "authors":
    {
        "name": "Gene Kong",
        "email": "gyx_edu@qq.com",
        "url": "https://github.com/FastEmbedded/FEmbed-OSlib.git"
    },
    "repository":
    {
        "type": "git",
        "url": "https://github.com/FastEmbedded/FEmbed-OSlib.git"
    },
    "version": "0.1.0",
    "frameworks": "esp8266-rtos-sdk",
    "platforms": "custom8266",
    "build": {
        "includeDir": "src",
        "srcFilter":[
            "-<freertos/*.c>",
            "-<port>",
            "+<port/freertos/Common>",
            "+<*.c>",
            "+<*.cpp>",
            "+<*.h>"
        ],
        "extraScript": "generate_headers.py"
    }
}